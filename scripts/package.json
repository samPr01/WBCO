{
  "name": "wbco-deploy-scripts",
  "version": "1.0.0",
  "description": "Deployment and testing scripts for WBCO project",
  "main": "deploy-test.js",
  "scripts": {
    "deploy-test": "node deploy-test.js",
    "update-wallets": "node update-wallets.js",
    "deploy-build": "node deploy-build.js",
    "deploy-build:dry": "node deploy-build.js --dry-run",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint *.js",
    "format": "prettier --write *.js"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "fs-extra": "^11.1.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "*.js",
      "!*.spec.js"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "keywords": [
    "deployment",
    "testing",
    "blockchain",
    "crypto",
    "wallet"
  ],
  "author": "WBCO Team",
  "license": "MIT"
}
